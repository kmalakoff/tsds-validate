    "tsds-build": "^1.20.4",
    "tsds-lib": "*",
    "tsds-typedoc": "*"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "mocha": "*",
    "mocha-compat": "*",
    "module-link-unlink": "*",
    "node-version-use": "*",
    "os-shim": "*",
    "resolve": "*",
    "short-hash": "*",
    "ts-dev-stack": "*",
    "ts-swc-loaders": "*",
    "tsds-config": "*",
    "tsds-lib-test": "*"
  },
  "engines": {
    "node": ">=0.8"

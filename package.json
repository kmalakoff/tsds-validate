    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "cross-spawn-cb": "*",
    "depcheck": "*",
    "queue-cb": "*",
    "resolve-bin-sync": "*",
    "sort-package-json": "*",
